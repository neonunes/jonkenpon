from random import randint
jogadas = 0
while True:
  jogadas+=1
  p1 = randint(1,3)
  p2 = randint(1,3)
  if p1==p2:
    print('empate')
    continue
  elif p1==1 and p2==2:
    print('venceu p2 1')
    jogadas==2
    
  elif p1==2 and p2==3:
    print('venceu p2 2')
    jogadas==2
    
  elif p1==3 and p2==1:
    print('venceu p2 3')
    jogadas==2
    break
  else:
    print('venceu p1 ',p1,p2)
    jogadas==2
